<?xml version="1.0"?>
<robot name="robotis_op2" xmlns:xacro="http://www.ros.org/wiki/xacro">

    <xacro:macro name="default_transmission" params="joint">
        <transmission name="transmission_${joint}">
            <plugin>transmission_interface/SimpleTransmission</plugin>
            <actuator name="${joint}" role="actuator1"/>
            <joint name="${joint}" role="joint1">
                <mechanical_reduction>1.0</mechanical_reduction>
            </joint>
        </transmission>
    </xacro:macro>
	
